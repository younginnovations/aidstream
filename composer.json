{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.1.17",
    "illuminate/html": "~5.0",
    "kris/laravel-form-builder": "1.6.20",
    "maatwebsite/excel": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "rap2hpoutre/laravel-log-viewer": "0.4.2",
    "barryvdh/laravel-ide-helper": "2.1.0",
    "barryvdh/laravel-debugbar": "2.0.6",
    "mockery/mockery": "^0.9.4",
    "yipl/hookah": "~0.4",
    "brianium/paratest": "~0.12",
    "laracasts/generators": "~1.1"
  },
  "autoload": {
    "files": [
      "app/Helpers/HelperLanguage.php"
    ],
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
